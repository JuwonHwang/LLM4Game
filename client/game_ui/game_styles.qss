QLabel {
    background-color:rgb(124, 124, 124);
    color: white;
    font-size: bold;
    padding: 24px;
    border-radius: 10px;
    font-size: 24px;
}

QPushButton {
    background-color:#A4A4A4;
    color: white;
    font-size: 12px;
    min-width: 120px;
    max-width: 120px;
    min-height: 40px;
    max-height: 40px;
    padding: 4px;
    border-radius: 10px;
    border: 0px solid transparent;
    margin: 0px;
}

QProgressBar {
    border: 2px solid #1E88E5;
    border-radius: 5px;
    background-color: #BBDEFB;
    text-align: center;
    font-size: 14px;
}

QProgressBar::chunk {
    background: qlineargradient(
        spread:pad, x1:0, y1:0, x2:1, y2:1,
        stop:0 #64B5F6, stop:1 #1E88E5
    );
    border-radius: 5px;
}